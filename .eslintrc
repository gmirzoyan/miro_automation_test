{
  "plugins": [ "webdriverio" ],
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "webextensions": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [ "error", {
      "code": 125,
      "tabWidth": 4
    } ],
    "max-lines": [ "error", 500 ]
  }
}